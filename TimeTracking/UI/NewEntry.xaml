<UserControl x:Class="TimeTracking.UI.NewEntry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimeTracking.UI"
        xmlns:i="clr-namespace:FeserWard.Controls;assembly=IntelliBox"
        xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
        mc:Ignorable="d"
        d:DesignHeight="50"  d:DesignWidth="100">
    <Grid>
        <i:Intellibox Name="tbKunde" WatermarkText="Kunde" SelectedValueBinding="{Binding}" DisplayedValueBinding="{Binding Kunde}">
            <i:Intellibox.Columns>
                <i:IntelliboxColumn  DisplayMemberBinding="{Binding Kunde}"></i:IntelliboxColumn>
            </i:Intellibox.Columns>
        </i:Intellibox>
    </Grid>
</UserControl>

