<UserControl x:Class="TimeTracking.UI.DayControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TimeTracking.UI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="White">
    <Grid>
        <DockPanel>
            <Button DockPanel.Dock="Top" Name="btNewEntry" Content="New Entry" Click="btNewEntry_Click"></Button>
            <StackPanel Name="MainStackpanel"></StackPanel>
        </DockPanel>
            
    </Grid>
</UserControl>
